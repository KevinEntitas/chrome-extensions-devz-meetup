{
    "manifest_version": 3,
    "name": "Finder helper - demo",
    "version": "1.0.0",
    "description": "DevZ meeting demo extension",
    "permissions": ["contextMenus", "tabs", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "js": ["content.js"],
            "css": ["styles.css"],
            "matches": ["<all_urls>"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    }
}
